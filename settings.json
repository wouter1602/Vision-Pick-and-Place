{
    "verbose": true,
    "capture_device": "/dev/video0",
    "detection_settings": {
        "edge_detection_type": 1,
        "canny_threshold_A": 90.0,
        "canny_threshold_B": 162.0,
        "contour_threshold_A": 176.0,
        "contour_threshold_B": 255.0,
        "blob_threshold_A": 255,
        "blob_threshold_B": 255,
        "blur_threshold_A": 10,
        "blur_threshold_B": 10,
        "dilate_iterations": 1,
        "erode_iterations": 1,
        "contour_poly_threshold": 0.01
    },
    "connection": {
        "TCP_IP": "127.0.0.1",
        "TCP_PORT": 5000
    },
    "use_test_image": false,
    "test_image": "./Image/Test_Img_Resized.jpg",
    "display_output": {
        "enable": true,
        "image_height": 360,
        "image_width": 480,
        "autodetect": 1
    }
}